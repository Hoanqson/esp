	uiStroke.Color = Color3.fromRGB(0, 200, 255);
	uiStroke.Thickness = 2;
	uiStroke.Parent = frame;
	local title = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\195\246\36\198\219\242\62\215\251", "\178\151\147\92"));
	title.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\184\244\88\62\23", "\26\236\157\44\82\114\44");
	title.Size = UDim2.new(1, 0, 0, 35);
	title.BackgroundColor3 = Color3.fromRGB(10, 10, 40);
	title.BackgroundTransparency = 0.1;
	title.TextColor3 = CONFIG.guiTitleColor;
	title.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\15\29\229\27\40\55\149\115\37\47\219\74\57\33\219\9\123\126\130", "\59\74\78\181");
	title.Font = Enum.Font.GothamBold;
	title.TextSize = 20;
	title.Parent = frame;
	local titleCorner = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\16\248\121\85\161\43\212\72", "\211\69\177\58\58"));
