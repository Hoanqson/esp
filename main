		success = true;
		if CONFIG.debugMode then
			print("ÄĂ£ Ä‘áº·t GUI vĂ o PlayerGui");
		end
	end);
	if not success then
		pcall(function()
			screenGui.Parent = game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\134\165\49\27\52\17\206", "\49\197\202\67\126\115\100\167"));
			success = true;
