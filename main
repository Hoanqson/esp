	titleCorner.Parent = title;
	local textBox = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\131\224\97\225\203\196\175", "\171\215\133\25\149\137"));
	textBox.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\200\220\55\247\193\49\241\71\200\198\34\239\251", "\34\129\168\82\154\143\80\156");
