		print("Äang táº¡o giao diá»‡n...");
	end
	local existingGui = player.PlayerGui:FindFirstChild(LUAOBFUSACTOR_DECRYPT_STR_0("\193\66\76\106\212\65\244\246\126\112\110\238\102", "\128\132\17\28\41\187\47"));
	if existingGui then
		existingGui:Destroy();
	end
	local screenGui = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\50\49\20\63\88\15\21\19\51", "\61\97\82\102\90"));
