local espEnabled = false;
local allESPObjects = {};
local foundItems = {};
local updateTask = nil;
local refreshTask = nil;
local function safeGetPosition(object)
	if (not object or (typeof(object) ~= LUAOBFUSACTOR_DECRYPT_STR_0("\113\80\22\61\38\227\17\93", "\114\56\62\101\73\71\141")) or not object:IsDescendantOf(game)) then
		return hrp.Position;
	end
	local position = nil;
	local success = pcall(function()
		if object:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\154\232\200\193\136\232\201\208", "\164\216\137\187")) then
			position = object.Position;
		elseif (object:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\255\233\53\183\170", "\107\178\134\81\210\198\158")) or object:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\12\1\141\202", "\202\88\110\226\166")) or object:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\226\12\129\242\217\208\0\144\238", "\170\163\111\226\151"))) then
			position = object:GetPivot().Position;
		elseif object.PrimaryPart then
			position = object.PrimaryPart.Position;
		else
			for _, child in ipairs(object:GetDescendants()) do
				if child:IsA(LUAOBFUSACTOR_DECRYPT_STR_0("\51\49\161\61\126\54\59\5", "\73\113\80\210\88\46\87")) then
					position = child.Position;
					break;
				end
			end
		end
	end);
	if (not success or not position) then
		return hrp.Position;
	end
	return position;
end
local function createESP(part, name)
	if (not part or not part:IsDescendantOf(game)) then
		return nil, nil;
	end
	local position = safeGetPosition(part);
	local distance = (hrp.Position - position).Magnitude;
	local billboard = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\163\37\193\30\229\142\45\223\22\192\148\37", "\135\225\76\173\114"));
	billboard.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\51\249\189\189\137\142\151", "\199\122\141\216\208\204\221");
	billboard.AlwaysOnTop = true;
	billboard.Size = CONFIG.espSize;
	billboard.StudsOffset = Vector3.new(0, 2, 0);
	billboard.Adornee = part;
	local label = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\153\216\8\228\84\247\175\216\28", "\150\205\189\112\144\24"));
	label.BackgroundTransparency = 1;
	label.Size = UDim2.new(1, 0, 1, 0);
	label.Text = name .. LUAOBFUSACTOR_DECRYPT_STR_0("\101\191", "\112\69\228\223\44\100\232\113") .. math.floor(distance) .. LUAOBFUSACTOR_DECRYPT_STR_0("\217\34", "\230\180\127\103\179\214\28");
	label.TextColor3 = CONFIG.espColor;
	label.TextStrokeColor3 = CONFIG.espTextStrokeColor;
	label.TextStrokeTransparency = CONFIG.espTextStrokeTransparency;
	label.TextSize = CONFIG.espTextSize;
	label.Font = CONFIG.espFont;
	label.TextScaled = false;
	label.Parent = billboard;
	local parent = game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\175\10\77\67\195\84\233", "\128\236\101\63\38\132\33"));
	pcall(function()
		billboard.Parent = parent;
	end);
	table.insert(allESPObjects, billboard);
	return billboard, label;
end
local function updateESPObjects()
	local i = 1;
	while i <= #allESPObjects do
		local object = allESPObjects[i];
		if (not object or not object.Parent) then
			table.remove(allESPObjects, i);
		else
			i = i + 1;
		end
	end
	i = 1;
	while i <= #foundItems do
		local item = foundItems[i];
		if (not item or not item.part or not item.part:IsDescendantOf(game) or not item.billboard or not item.billboard.Parent) then
			if (item and item.billboard and item.billboard.Parent) then
				item.billboard:Destroy();
			end
			table.remove(foundItems, i);
		else
			i = i + 1;
		end
	end
end
local function findItems()
	if not espEnabled then
		return {};
	end
	updateESPObjects();
	local count = 0;
	local newItems = {};
	local foundItemsMap = {};
	for _, item in ipairs(foundItems) do
		if (item.part and item.part:IsDescendantOf(game) and item.billboard and item.billboard.Parent) then
			local keep = false;
			for _, targetName in ipairs(CONFIG.targetNames) do
				if (item.part.Name == targetName) then
					local position = safeGetPosition(item.part);
					local distance = (hrp.Position - position).Magnitude;
					if (distance <= CONFIG.maxDistance) then
						foundItemsMap[item.part] = true;
						table.insert(newItems, item);
						keep = true;
						count = count + 1;
						break;
					end
				end
			end
			if (not keep and item.billboard) then
				pcall(function()
					item.billboard:Destroy();
				end);
			end
		end
	end
	local function searchInModel(model)
		if not espEnabled then
			return;
		end
		for _, v in pairs(model:GetChildren()) do
			if not espEnabled then
				return;
			end
			for _, targetName in ipairs(CONFIG.targetNames) do
				if ((v.Name == targetName) and not foundItemsMap[v]) then
					local position = safeGetPosition(v);
					local distance = (hrp.Position - position).Magnitude;
					if (distance <= CONFIG.maxDistance) then
						count = count + 1;
						if CONFIG.debugMode then
							print("ÄĂ£ tĂ¬m tháº¥y " .. v.Name .. LUAOBFUSACTOR_DECRYPT_STR_0("\236\234", "\175\204\201\113\36\214\139") .. count .. " táº¡i vá»‹ trĂ­: " .. tostring(position) .. " (Khoáº£ng cĂ¡ch: " .. math.floor(distance) .. LUAOBFUSACTOR_DECRYPT_STR_0("\74\133", "\100\39\172\85\188"));
						end
						local billboard, label = createESP(v, v.Name);
						if (billboard and label) then
							foundItemsMap[v] = true;
							table.insert(newItems, {[LUAOBFUSACTOR_DECRYPT_STR_0("\189\121\171\148", "\83\205\24\217\224")]=v,[LUAOBFUSACTOR_DECRYPT_STR_0("\228\204\193\49\228\202\204\47\226", "\93\134\165\173")]=billboard,[LUAOBFUSACTOR_DECRYPT_STR_0("\178\243\195\199\54", "\30\222\146\161\162\90\174\210")]=label,[LUAOBFUSACTOR_DECRYPT_STR_0("\233\79\99\30\213\65\99\3\241\71\127\4", "\106\133\46\16")]=position});
						end
					end
				end
			end
			if (#v:GetChildren() > 0) then
				searchInModel(v);
			end
		end
	end
	pcall(function()
		searchInModel(game.Workspace);
	end);
	if CONFIG.debugMode then
		if (count > 0) then
			print("ÄĂ£ tĂ¬m tháº¥y tá»•ng cá»™ng " .. count .. " váº­t pháº©m phĂ¹ há»£p");
		else
			print("KhĂ´ng tĂ¬m tháº¥y váº­t pháº©m nĂ o phĂ¹ há»£p trong Workspace");
		end
	end
	return newItems;
end
local function updateESPLabels()
	if not espEnabled then
		return;
	end
	updateESPObjects();
	for _, item in ipairs(foundItems) do
		if (item.part and item.part:IsDescendantOf(game) and item.label and item.label.Parent) then
			pcall(function()
				local position = safeGetPosition(item.part);
				local distance = (hrp.Position - position).Magnitude;
				if (distance <= CONFIG.maxDistance) then
					item.label.Text = item.part.Name .. LUAOBFUSACTOR_DECRYPT_STR_0("\24\27", "\32\56\64\19\156\58") .. math.floor(distance) .. LUAOBFUSACTOR_DECRYPT_STR_0("\87\245", "\224\58\168\133\54\58\146");
					item.lastPosition = position;
					item.billboard.Enabled = true;
				else
					item.billboard.Enabled = false;
				end
			end);
		end
	end
end
local function cleanup()
	if updateTask then
		task.cancel(updateTask);
		updateTask = nil;
	end
	if refreshTask then
		task.cancel(refreshTask);
		refreshTask = nil;
	end
	for _, object in ipairs(allESPObjects) do
		if (object and object.Parent) then
			pcall(function()
				object:Destroy();
			end);
		end
	end
	for _, obj in pairs(game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\122\89\89\248\82\147\142", "\107\57\54\43\157\21\230\231")):GetChildren()) do
		if (obj.Name == LUAOBFUSACTOR_DECRYPT_STR_0("\242\159\20\248\156\239\255", "\175\187\235\113\149\217\188")) then
			pcall(function()
				obj:Destroy();
			end);
		end
	end
	allESPObjects = {};
	foundItems = {};
	if CONFIG.debugMode then
		print("ÄĂ£ dá»n dáº¹p táº¥t cáº£ cĂ¡c Ä‘á»‘i tÆ°á»£ng ESP");
	end
end
local function enableESP()
	if espEnabled then
		return;
	end
	cleanup();
	espEnabled = true;
	foundItems = findItems();
	pcall(function()
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\15\187\128\94\247\124\106\27\186\136", "\24\92\207\225\44\131\25")):SetCore(LUAOBFUSACTOR_DECRYPT_STR_0("\120\214\182\72\53\114\95\218\190\69\24\124\95\218\183\66", "\29\43\179\216\44\123"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\137\208\52\64\184", "\44\221\185\64")]=LUAOBFUSACTOR_DECRYPT_STR_0("\36\212\120\31\90\21\226\69", "\19\97\135\40\63"),[LUAOBFUSACTOR_DECRYPT_STR_0("\154\89\43\47", "\81\206\60\83\91\79")]=("ÄĂ£ báº­t ESP cho " .. table.concat(CONFIG.targetNames, LUAOBFUSACTOR_DECRYPT_STR_0("\2\235", "\196\46\203\176\18\79\163\45"))),[LUAOBFUSACTOR_DECRYPT_STR_0("\156\55\108\31\48\242\224\182", "\143\216\66\30\126\68\155")]=CONFIG.notificationDuration});
	end);
	updateTask = task.spawn(function()
		while espEnabled do
			if hrp then
				pcall(function()
					updateESPLabels();
				end);
			end
			task.wait(CONFIG.updateInterval);
		end
	end);
	refreshTask = task.spawn(function()
		while espEnabled do
			if hrp then
				pcall(function()
					local newItems = findItems();
					if (#newItems > 0) then
						foundItems = newItems;
					end
				end);
			end
			task.wait(CONFIG.refreshInterval);
		end
	end);
end
local function disableESP()
	if not espEnabled then
		return;
	end
	espEnabled = false;
	cleanup();
	pcall(function()
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\153\220\12\217\209\166\197\198\191\193", "\129\202\168\109\171\165\195\183")):SetCore(LUAOBFUSACTOR_DECRYPT_STR_0("\17\93\57\220\240\27\242\43\94\62\219\223\0\239\45\86", "\134\66\56\87\184\190\116"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\8\56\29\183\28", "\85\92\81\105\219\121\139\65")]=LUAOBFUSACTOR_DECRYPT_STR_0("\216\128\96\5\85\203\248\190", "\191\157\211\48\37\28"),[LUAOBFUSACTOR_DECRYPT_STR_0("\235\26\236\8", "\90\191\127\148\124")]="ÄĂ£ táº¯t ESP",[LUAOBFUSACTOR_DECRYPT_STR_0("\92\146\60\22\108\142\33\25", "\119\24\231\78")]=CONFIG.notificationDuration});
	end);
end
local function setTargetNames(names)
	if (type(names) == LUAOBFUSACTOR_DECRYPT_STR_0("\145\57\183\67\210\71", "\113\226\77\197\42\188\32")) then
		CONFIG.targetNames = {names};
	elseif (type(names) == LUAOBFUSACTOR_DECRYPT_STR_0("\46\23\246\185\63", "\213\90\118\148")) then
		CONFIG.targetNames = names;
	end
	if espEnabled then
		disableESP();
		task.wait(0.1);
		enableESP();
	end
	pcall(function()
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\104\58\181\68\89\94\60\147\67\68", "\45\59\78\212\54")):SetCore(LUAOBFUSACTOR_DECRYPT_STR_0("\35\83\141\143\168\33\185\249\22\95\128\138\146\39\162\254", "\144\112\54\227\235\230\78\205"), {[LUAOBFUSACTOR_DECRYPT_STR_0("\135\33\27\240\213", "\59\211\72\111\156\176")]=LUAOBFUSACTOR_DECRYPT_STR_0("\107\180\211\109\103\147\230\32", "\77\46\231\131"),[LUAOBFUSACTOR_DECRYPT_STR_0("\142\81\174\84", "\32\218\52\214")]=("ÄĂ£ Ä‘áº·t má»¥c tiĂªu: " .. table.concat(CONFIG.targetNames, LUAOBFUSACTOR_DECRYPT_STR_0("\2\87", "\58\46\119\81\200\145\208\37"))),[LUAOBFUSACTOR_DECRYPT_STR_0("\15\153\34\173\189\180\57\37", "\86\75\236\80\204\201\221")]=CONFIG.notificationDuration});
	end);
end
local userInputService = game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\71\82\114\151\215\133\98\84\99\182\251\153\100\72\116\128", "\235\18\33\23\229\158"));
userInputService.InputBegan:Connect(function(input, gameProcessedEvent)
	if not gameProcessedEvent then
		if (input.KeyCode == CONFIG.toggleKey) then
			if espEnabled then
				disableESP();
			else
				enableESP();
			end
		end
	end
end);
player.CharacterAdded:Connect(function(newCharacter)
	character = newCharacter;
	hrp = character:WaitForChild(LUAOBFUSACTOR_DECRYPT_STR_0("\120\175\204\186\94\181\200\191\98\181\206\175\96\187\211\175", "\219\48\218\161"));
	if espEnabled then
		disableESP();
		task.wait(0.5);
		enableESP();
	else
		cleanup();
	end
end);
local function createGUI()
