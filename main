	textBox.Position = UDim2.new(0.05, 0, 0.37, 0);
	textBox.BackgroundColor3 = Color3.fromRGB(60, 60, 80);
	textBox.TextColor3 = Color3.fromRGB(255, 255, 255);
	textBox.PlaceholderText = LUAOBFUSACTOR_DECRYPT_STR_0("\172\166\54\6\8\64\136\136\183\115\67\75\65\132\136\179\115\24\77\94\136\151\179\39\14\76\7", "\233\229\210\83\107\40\46");
	textBox.PlaceholderColor3 = Color3.fromRGB(200, 200, 200);
	textBox.Text = table.concat(CONFIG.targetNames, LUAOBFUSACTOR_DECRYPT_STR_0("\141\2", "\101\161\34\82\182"));
	textBox.Font = Enum.Font.Gotham;
	textBox.TextSize = 14;
	textBox.ClearTextOnFocus = false;
	textBox.Parent = frame;
	local textBoxCorner = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\221\36\122\241\201\236\135\60", "\78\136\109\57\158\187\130\226"));
	textBoxCorner.CornerRadius = UDim.new(0, 6);
	textBoxCorner.Parent = textBox;
	local applyButton = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\10\58\225\229\28\42\237\229\49\49", "\145\94\95\153"));
	applyButton.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\220\221\4\217\87\149\232\217\0\218\64", "\215\157\173\116\181\46");
	applyButton.Size = UDim2.new(0.45, 0, 0, 30);
	applyButton.Position = UDim2.new(0.03, 0, 0.68, 0);
	applyButton.BackgroundColor3 = CONFIG.guiButtonColor;
	applyButton.TextColor3 = Color3.fromRGB(255, 255, 255);
	applyButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\20\164\155\254\195", "\186\85\212\235\146");
	applyButton.Font = Enum.Font.GothamBold;
	applyButton.TextSize = 16;
	applyButton.Parent = frame;
	local applyCorner = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\247\168\53\241\43\224\93\208", "\56\162\225\118\158\89\142"));
	applyCorner.CornerRadius = UDim.new(0, 6);
	applyCorner.Parent = applyButton;
	local toggleButton = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\104\0\216\187\0\205\72\17\207\161", "\184\60\101\160\207\66"));
	toggleButton.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\5\141\123\187\61\135\94\169\37\150\115\178", "\220\81\226\28");
	toggleButton.Size = UDim2.new(0.45, 0, 0, 30);
	toggleButton.Position = UDim2.new(0.52, 0, 0.68, 0);
	toggleButton.BackgroundColor3 = (espEnabled and CONFIG.guiToggleOnColor) or CONFIG.guiToggleOffColor;
	toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255);
	toggleButton.Text = (espEnabled and LUAOBFUSACTOR_DECRYPT_STR_0("\55\220\145\250\232\203\22\149\167\200\218", "\167\115\181\226\155\138")) or LUAOBFUSACTOR_DECRYPT_STR_0("\199\44\230\94\119\116\134\199\17\215", "\166\130\66\135\60\27\17");
	toggleButton.Font = Enum.Font.GothamBold;
	toggleButton.TextSize = 16;
	toggleButton.Parent = frame;
	local toggleCorner = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\113\99\237\122\34\74\79\220", "\80\36\42\174\21"));
	toggleCorner.CornerRadius = UDim.new(0, 6);
	toggleCorner.Parent = toggleButton;
	local applyHoverEffect = function(isHovering)
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\122\7\50\127\64\35\50\104\88\25\52\127", "\26\46\112\87")):Create(applyButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {[LUAOBFUSACTOR_DECRYPT_STR_0("\155\34\168\127\184\173\74\161\183\39\136\123\179\176\87\231", "\212\217\67\203\20\223\223\37")]=((isHovering and Color3.fromRGB(0, 200, 255)) or CONFIG.guiButtonColor)}):Play();
	end;
	applyButton.MouseEnter:Connect(function()
		applyHoverEffect(true);
	end);
	applyButton.MouseLeave:Connect(function()
		applyHoverEffect(false);
	end);
	local toggleHoverEffect = function(isHovering)
		local baseColor = (espEnabled and CONFIG.guiToggleOnColor) or CONFIG.guiToggleOffColor;
		local hoverColor = (espEnabled and Color3.fromRGB(255, 70, 70)) or Color3.fromRGB(70, 220, 70);
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\142\154\173\215\180\190\173\192\172\132\171\215", "\178\218\237\200")):Create(toggleButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {[LUAOBFUSACTOR_DECRYPT_STR_0("\148\180\229\219\177\167\233\197\184\177\197\223\186\186\244\131", "\176\214\213\134")]=((isHovering and hoverColor) or baseColor)}):Play();
	end;
	toggleButton.MouseEnter:Connect(function()
		toggleHoverEffect(true);
	end);
	toggleButton.MouseLeave:Connect(function()
		toggleHoverEffect(false);
	end);
	local function buttonPressEffect(button)
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\192\186\179\209\166\101\92\230\187\191\215\173", "\57\148\205\214\180\200\54")):Create(button, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {[LUAOBFUSACTOR_DECRYPT_STR_0("\33\244\47\49", "\22\114\157\85\84")]=UDim2.new(button.Size.X.Scale, button.Size.X.Offset, 0, button.Size.Y.Offset - 2)}):Play();
		task.wait(0.1);
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\240\220\22\193\83\197\173\214\221\26\199\88", "\200\164\171\115\164\61\150")):Create(button, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {[LUAOBFUSACTOR_DECRYPT_STR_0("\141\253\25\64", "\227\222\148\99\37")]=UDim2.new(button.Size.X.Scale, button.Size.X.Offset, 0, button.Size.Y.Offset + 2)}):Play();
	end
	applyButton.MouseButton1Click:Connect(function()
		buttonPressEffect(applyButton);
		local text = textBox.Text;
		local names = {};
		text = string.gsub(text, LUAOBFUSACTOR_DECRYPT_STR_0("\8\108\23\225\188\127\23\65\203", "\153\83\50\50\150"), "");
		for name in string.gmatch(text, LUAOBFUSACTOR_DECRYPT_STR_0("\102\72\63\33\56", "\45\61\22\19\124\19\203")) do
			local trimmedName = string.match(name, LUAOBFUSACTOR_DECRYPT_STR_0("\255\87\30\191\74\62\244\136\87\30\191\70", "\217\161\114\109\149\98\16"));
			name = trimmedName or name;
			if (name and (name ~= "")) then
				table.insert(names, name);
			end
		end
		if (#names > 0) then
			setTargetNames(names);
		end
	end);
	toggleButton.MouseButton1Click:Connect(function()
		buttonPressEffect(toggleButton);
		if espEnabled then
			disableESP();
			toggleButton.BackgroundColor3 = CONFIG.guiToggleOffColor;
			toggleButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\55\46\57\126\176\113\82\5\11\76", "\20\114\64\88\28\220");
		else
			enableESP();
			toggleButton.BackgroundColor3 = CONFIG.guiToggleOnColor;
			toggleButton.Text = LUAOBFUSACTOR_DECRYPT_STR_0("\21\8\193\181\250\220\184\113\36\225\132", "\221\81\97\178\212\152\176");
		end
	end);
	local keyInfo = Instance.new(LUAOBFUSACTOR_DECRYPT_STR_0("\249\226\5\239\54\204\229\24\247", "\122\173\135\125\155"));
	keyInfo.Name = LUAOBFUSACTOR_DECRYPT_STR_0("\175\196\25\144\49\55\199", "\168\228\161\96\217\95\81");
	keyInfo.Size = UDim2.new(1, 0, 0, 20);
	keyInfo.Position = UDim2.new(0, 0, 0.9, 0);
	keyInfo.BackgroundTransparency = 1;
	keyInfo.TextColor3 = Color3.fromRGB(200, 200, 200);
	keyInfo.Text = "PhĂ­m táº¯t: " .. CONFIG.toggleKey.Name;
	keyInfo.Font = Enum.Font.Gotham;
	keyInfo.TextSize = 12;
	keyInfo.Parent = frame;
	local dragging = false;
	local dragInput;
	local dragStart;
	local startPos;
	local function updateDrag(input)
		local delta = input.Position - dragStart;
		local newPosition = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y);
		game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\239\198\43\89\33\100\222\195\56\85\44\82", "\55\187\177\78\60\79")):Create(frame, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {[LUAOBFUSACTOR_DECRYPT_STR_0("\29\193\76\226\82\198\143\35", "\224\77\174\63\139\38\175")]=newPosition}):Play();
	end
	title.InputBegan:Connect(function(input)
		if ((input.UserInputType == Enum.UserInputType.MouseButton1) or (input.UserInputType == Enum.UserInputType.Touch)) then
			dragging = true;
			dragStart = input.Position;
			startPos = frame.Position;
			uiStroke.Color = Color3.fromRGB(0, 255, 255);
			uiStroke.Thickness = 3;
			input.Changed:Connect(function()
				if (input.UserInputState == Enum.UserInputState.End) then
					dragging = false;
					uiStroke.Color = Color3.fromRGB(0, 200, 255);
					uiStroke.Thickness = 2;
				end
			end);
		end
	end);
	title.InputChanged:Connect(function(input)
		if ((input.UserInputType == Enum.UserInputType.MouseMovement) or (input.UserInputType == Enum.UserInputType.Touch)) then
			dragInput = input;
		end
	end);
	userInputService.InputChanged:Connect(function(input)
		if ((input == dragInput) and dragging) then
			updateDrag(input);
		end
	end);
end
game:GetService(LUAOBFUSACTOR_DECRYPT_STR_0("\180\77\89\55\129\83\75", "\78\228\33\56")).PlayerRemoving:Connect(function(plr)
	if (plr == player) then
		cleanup();
	end
end);
script.Destroying:Connect(function()
	cleanup();
end);
createGUI();
